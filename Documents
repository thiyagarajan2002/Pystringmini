#Thiyagarajan V
from pystringmini import *
string="Hello Hai"
print(pystringmini.uppercase(string))
print(pystringmini.lowercase(string))
print(pystringmini.capitalize(string))
print(pystringmini.length(string))
print(pystringmini.digit(string))
print(pystringmini.minimum(string))
print(pystringmini.maximum(string))
print(pystringmini.replace(string,'Hai','Everyone'))
print(pystringmini.encryption(string))
print(pystringmini.decryption(string))
print(pystringmini.split(string))
print(pystringmini.helps())
print(pystringmini.about())
